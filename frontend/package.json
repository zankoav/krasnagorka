{
    "name": "frontend",
    "version": "0.0.0",
    "author": "zankoav",
    "bugs": "https://github.com/zankoav/frontend/issues",
    "dependencies": {
        "lwc-services": "^1.3.4",
        "@babel/core": "^7.5.5",
        "@lwc/engine": "^1.0.0",
        "@salesforce/eslint-config-lwc": "^0.3.0",
        "@salesforce/lwc-jest": "^0.5.4",
        "@salesforce/sfdx-lwc-jest": "^0.5.4",
        "assets-webpack-plugin": "^3.9.10",
        "autoprefixer": "^9.6.1",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.5",
        "babel-preset-env": "^1.7.0",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^3.2.0",
        "eslint": "^5.16.0",
        "eslint-config-airbnb-base": "^13.2.0",
        "eslint-config-prettier": "^6.0.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.18.0",
        "eslint-plugin-prettier": "^3.1.0",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "html-webpack-plugin": "^3.2.0",
        "identity-obj-proxy": "^3.0.0",
        "jest-cli": "^24.9.0",
        "lwc-webpack-plugin": "^0.2.0",
        "node-sass": "^4.12.0",
        "postcss-loader": "^3.0.0",
        "prettier": "1.18.2",
        "resolve-url-loader": "^3.1.0",
        "sass-loader": "^8.0.0",
        "style-loader": "^1.0.0",
        "webpack": "^4.35.2",
        "webpack-cli": "^3.3.5",
        "webpack-dev-server": "^3.7.2",
        "pug": "^2.0.4",
        "pug-loader": "^2.4.0"
    },
    "devDependencies": {
        "husky": "^3.0.9",
        "lint-staged": "^9.4.2"
    },
    "engines": {
        "node": ">=10.0.0 <11.0.0"
    },
    "homepage": "https://github.com/zankoav/frontend",
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "keywords": [
        "lwc"
    ],
    "license": "MIT",
    "lint-staged": {
        "**/*.{css,html,js,json,md,ts,yaml,yml}": [
            "prettier --write"
        ],
        "./src/**/*.js": [
            "eslint"
        ],
        "*": [
            "git add"
        ]
    },
    "repository": "zankoav/frontend",
    "scripts": {
        "build": "lwc-services build -m production",
        "build:development": "lwc-services build",
        "lint": "eslint ./src/**/*.js",
        "prettier": "prettier --write '**/*.{css,html,js,json,md,ts,yaml,yml}'",
        "prettier:verify": "prettier --list-different '**/*.{css,html,js,json,md,ts,yaml,yml}'",
        "serve": "lwc-services serve",
        "test:unit": "lwc-services test:unit",
        "test:unit:coverage": "lwc-services test:unit --coverage",
        "test:unit:debug": "lwc-services test:unit --debug",
        "test:unit:watch": "lwc-services test:unit --watch",
        "watch": "lwc-services watch",
        "FrontBuild": "webpack --env.MODE=production",
        "FrontWatch": "webpack-dev-server --env.MODE=development --open"
    }
}
